/print-draft {
	gsave
		-45 rotate
		-100 0 moveto
		/Helvetica-Bold findfont
			[ 120.0 0 0 -120.0 0 0 ] makefont setfont
		(DRAFT) show
	grestore
	gsave
		300 -100 moveto
